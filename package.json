{
  "name": "audioteca-critica",
  "description": "Website to track listened episodes of the podcast Audioteca Crítica",
  "version": "1.0.0",
  "private": true,
  "author": "Azhariel <azhariel@gmail.com> (https://github.com/azhariel) . Starter template by João Pedro Schmitz <hey@joaopedro.dev> (https://joaopedro.dev)",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "starter",
    "typescript"
  ],
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "test": "vitest",
    "test:watch": "vitest watch"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@fontsource/roboto": "5.0.13",
    "@mui/icons-material": "5.11.16",
    "@mui/material": "5.11.16",
    "@mui/styled-engine-sc": "5.11.11",
    "@vercel/analytics": "0.1.11",
    "babel-plugin-styled-components": "2.1.1",
    "dotenv": "16.0.3",
    "gatsby": "5.12.8",
    "gatsby-plugin-image": "3.8.0",
    "gatsby-plugin-manifest": "5.12.3",
    "gatsby-plugin-pnpm": "1.2.10",
    "gatsby-plugin-sharp": "5.12.3",
    "gatsby-plugin-styled-components": "6.8.0",
    "gatsby-source-contentful": "8.12.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "6.1.8",
    "tsconfig-paths-webpack-plugin": "4.1.0"
  },
  "devDependencies": {
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.21.4",
    "@commitlint/cli": "17.5.1",
    "@commitlint/config-conventional": "17.4.4",
    "@testing-library/dom": "9.2.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/react-hooks": "8.0.1",
    "@types/node": "18.15.11",
    "@types/react": "18.0.32",
    "@types/react-dom": "18.0.11",
    "@types/styled-components": "5.1.26",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "@vitejs/plugin-react": "3.1.0",
    "babel-preset-gatsby": "3.8.0",
    "c8": "10.1.2",
    "eslint": "9.9.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard": "17.0.0",
    "eslint-import-resolver-typescript": "3.5.4",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jsdom": "23.2.0",
    "lint-staged": "15.2.9",
    "prettier": "3.3.3",
    "typescript": "5.0.3",
    "vite-tsconfig-paths": "4.0.7",
    "vitest": "0.29.8"
  }
